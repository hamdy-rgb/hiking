{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "quotes": ["error", "double"],
    "import/no-unresolved": ["off"],
    "import/prefer-default-export": ["off"],
    "operator-linebreak": "off",
    "linebreak-style": 0,
    "object-curly-newline": "off"
  }
}
